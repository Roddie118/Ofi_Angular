let num : number = 1

do {
    if (num % 3 == 0 && num % 5 == 0){
        console.log("divisivel por 3 e 5")
}   else if (num % 3 == 0){
        console.log("Ã© divisivel por 3")
}    else if (num % 5 == 0){
        console.log ("divisivel por 5")
}    else{
       console.log(num)
}    
    num++;
}while (num<=1000)
